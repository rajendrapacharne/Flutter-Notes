void main() {
  add(10,20);
  manager(5,'pants');
  int sum=sub(20,10);
  print(sum);
}

add(int a,int b)
{
  int sum=0;
  for (int i = 0; i < 5; i++) {
    sum=sum+i;
    print('hello ${i + 1}');
  }
  print(sum);
  print(a+b);
}

manager(int q,String item)
{
  print('$q of $item are ready');
}
int sub(int a,int b)=> a-b;